{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": ["es5"],
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "esModuleInterop": false,
		"moduleResolution": "node",
  },
	"exclude": ["node_modules"]
}